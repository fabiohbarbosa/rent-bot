    "cors": "^2.8.5",
    "esm": "^3.0.84",
    "express": "^4.16.4",
    "handlebars": "^4.7.6",
    "mongodb": "^3.1.10",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "tsconfig-paths": "^3.8.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
