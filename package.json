  "main": "index.js",
  "scripts": {
    "start": "node --inspect=9229 -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "start:dev": "nodemon",
    "start:prod": "npm run build && sh start.sh",
    "build:remove": "rm -rf dist",
    "build:assets": "mkdir dist && cp -a assets dist",
    "build:tsc": "tsc",
    "build": "npm run build:remove && npm run build:assets && npm rum build:tsc",
    "lint": "tslint -p tslint.json src/**/*.ts config/**/**.ts",
    "lint:fix": "tslint --fix -p tslint.json src/**/*.ts config/**/**.ts",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
